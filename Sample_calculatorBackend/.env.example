# Flask/Python environment configuration for Sample_calculatorBackend

# Required for Flask SECRET_KEY, session management, cryptography
SECRET_KEY=your_secret_key_here

# PostgreSQL connection string format:
# postgresql://username:password@host:port/database
POSTGRES_URL=postgresql://username:password@localhost:5432/sample_calculator_db

# Deployment configuration
FLASK_RUN_HOST=0.0.0.0
FLASK_RUN_PORT=5000

# For environment switching (development/production/testing)
ENVIRONMENT=development
